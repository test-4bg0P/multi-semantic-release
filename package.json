{
  "name": "@payfit/multi-semantic-release",
  "author": "PayFit",
  "version": "0.0.0-development",
  "license": "0BSD",
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.0.0"
  },
  "type": "module",
  "exports": "./dist/lib/multiSemanticRelease.js",
  "bin": {
    "multi-semantic-release": "./dist/bin/cli.js"
  },
  "files": [
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig-build.json",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ./",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:ci": "NODE_OPTIONS=--experimental-vm-modules GITHUB_REF=master GITHUB_EVENT_NAME=merge GITHUB_ACTIONS=true yarn test",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watchAll",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "prepublishOnly": "yarn build",
    "release": "semantic-release"
  },
  "dependencies": {
    "@manypkg/get-packages": "^1.1.1",
    "async-mutex": "^0.4.0",
    "batching-toposort": "^1.2.0",
    "blork": "^9.3.0",
    "cosmiconfig": "^8.0.0",
    "debug": "^4.3.2",
    "detect-indent": "^7.0.0",
    "detect-newline": "^4.0.0",
    "execa": "^7.0.0",
    "git-log-parser": "^1.2.0",
    "globby": "^13.0.0",
    "lodash-es": "^4.17.21",
    "meow": "^12.0.0",
    "promise-events": "^0.2.4",
    "semantic-release": "npm:@killianhmyd/semantic-release@0.0.0-notes-fix.10",
    "semver": "^7.3.5",
    "signale": "^1.4.0",
    "stream-buffers": "^3.0.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "17.8.1",
    "@schemastore/package": "0.0.8",
    "@types/cosmiconfig": "6.0.0",
    "@types/debug": "4.1.8",
    "@types/execa": "2.0.0",
    "@types/jest": "29.5.2",
    "@types/lodash-es": "4.17.7",
    "@types/node": "18.16.18",
    "@types/semantic-release": "20.0.1",
    "@types/semver": "7.5.0",
    "@types/signale": "1.4.4",
    "@types/stream-buffers": "3.0.4",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "commitlint": "17.8.1",
    "coveralls": "3.1.1",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-standard-with-typescript": "26.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-n": "16.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "file-url": "4.0.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "tempy": "3.0.0",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PayFit/multi-semantic-release.git"
  },
  "bugs": {
    "url": "https://github.com/PayFit/multi-semantic-release/issues"
  }
}
